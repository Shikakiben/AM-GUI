+quiet_cmd_regen_makefile = ACTION Regenerating $@
+cmd_regen_makefile = cd $(srcdir); /home/moi/AM-GUI/node_modules/@electron/node-gyp/gyp/gyp_main.py -fmake --ignore-environment "-Dlibrary=shared_library" "-Dvisibility=default" "-Dnode_root_dir=/home/moi/.electron-gyp/35.7.5" "-Dnode_gyp_dir=/home/moi/AM-GUI/node_modules/@electron/node-gyp" "-Dnode_lib_file=/home/moi/.electron-gyp/35.7.5/<(target_arch)/node.lib" "-Dmodule_root_dir=/home/moi/AM-GUI/node_modules/@homebridge/node-pty-prebuilt-multiarch" "-Dnode_engine=v8" "--depth=." "-Goutput_dir=." "--generator-output=build" -I/home/moi/AM-GUI/node_modules/@homebridge/node-pty-prebuilt-multiarch/build/config.gypi -I/home/moi/AM-GUI/node_modules/@electron/node-gyp/addon.gypi -I/home/moi/.electron-gyp/35.7.5/include/node/common.gypi "--toplevel-dir=." binding.gyp
